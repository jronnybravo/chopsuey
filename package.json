{
    "name": "chopsuey",
    "version": "0.0.4",
    "description": "Combinatorics: subsets, permutations, etc.",
    "main": "lib/ChopSuey.js",
    "types": "lib/ChopSuey.d.ts",
    "scripts": {
        "test": "jest --config jestconfig.json",
        "build": "tsc",
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
        "lint": "tslint -p tsconfig.json",
        "prepare": "npm run build",
        "prepublishOnly": "npm test && npm run lint",
        "preversion": "npm run lint",
        "version": "npm run format && git add -A src",
        "postversion": "git push && git push --tags"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jronnybravo/chopsuey.git"
    },
    "keywords": [
        "Combinatorics",
        "Combinations",
        "Subsets",
        "Permutations",
        "Shuffles"
    ],
    "author": "Jronny Amarante",
    "license": "Apache-2.0",
    "licenses": [
        {
            "type": "Apache-2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0"
        }
    ],
    "bugs": {
        "url": "https://github.com/jronnybravo/chopsuey/issues"
    },
    "homepage": "https://github.com/jronnybravo/chopsuey#readme",
    "devDependencies": {
        "@types/jest": "^27.0.2",
        "jest": "^27.2.5",
        "prettier": "^2.4.1",
        "ts-jest": "^27.0.5",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^4.4.3"
    },
    "files": [
        "lib/**/*"
    ]
